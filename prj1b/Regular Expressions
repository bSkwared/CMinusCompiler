/* Here is everything we need to recognize */
/* Regular expressions */
/* Project 1 regex
    (else | if | int | return | void | while | [a-zA-Z]([a-zA-Z])* | [0-9][0-9]* | /\*.*\*/ | != | <(=|e) | >(=|e) | =(=|e) | [+\-\*/(){}\[\]])*
*/

ELSE_RG = "else";
IF_RG = "if";
INT_RG = "int";
RET_RG = "return";
VOID_RG = "void";
WHILE_RG = "while";

letter = "[a-z]|[A-Z]";
digit = "[0-9]";

ID = "letter(letter)*"
NUM = "digit(digit)*";
